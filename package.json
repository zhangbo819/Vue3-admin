{
  "name": "vue3-admin",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "rm -rf docs && vue-tsc --noEmit && vite build --base=/Vue3-admin/",
    "preview": "vite preview",
    "eslint:comment": "使用 ESLint 检查并自动修复 src 目录下所有扩展名为 .js 和 .vue 的文件",
    "eslint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "prettier:comment": "自动格式化当前目录下的所有文件",
    "prettier": "prettier --write src/",
    "publish": "pnpm run build && git add . && git commit -m 'publish' && git push"
  },
  "dependencies": {
    "echarts": "^5.3.3",
    "element-plus": "^2.2.21",
    "vconsole": "^3.14.7",
    "vue": "^3.2.25",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "less": "^4.1.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.35"
  }
}
